#include <stdio.h>

int Answer;
int bin;

void test(int num) {

	if (num == 0) {
		return;
	}
	else {
		int put = num % 2;
		num /= 2;

		test(num);

		bin *= 10;
		bin += put;

		return;
	}
}

int main(void)
{
	int T, test_case, num;

	setbuf(stdout, NULL);

	scanf("%d", &T);

	for (test_case = 0; test_case < T; test_case++)
	{
		scanf("%d", &num);

		test(num);

		Answer = 0;

		while(bin != 0){
			if (bin % 10 == 1) {
				Answer++;
			}

			bin /= 10;
		}

		printf("Case #%d\n", test_case + 1);
		printf("%d\n", Answer);

	}

	return 0;
}
